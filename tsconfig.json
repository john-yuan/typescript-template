{
  "include": [
    "src/**/*.ts"
  ],
  "compilerOptions": {
    "lib": ["DOM", "ES2016"],
    "target": "es5",
    "module": "commonjs",
    "outDir": "dist",
    "strict": true,
    "declaration": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
